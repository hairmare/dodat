{
    "name": "hairmare/dodat",
    "description": "Do Da Travis runs scripts from your travis yml",
    "bin": [
        "bin/dodat"
    ],
    "autoload": {
        "psr-4": { "Hairmare\\Dodat\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Hairmare\\Dodat\\": "dev/" }
    },
    "require": {
        "symfony/yaml": "~4.0",
        "symfony/process": "~2.6",
        "symfony/console": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "squizlabs/php_codesniffer": "~2.2"
    },
    "license": "GPL",
    "authors": [
        {
            "name": "Lucas Bickel",
            "email": "hairmare@purplehaze.ch"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev",
            "dev-develop": "1.x-dev"
        }
    },
    "non-feature-branches": [
        "master",
        "develop",
        "support/*"
    ]
}
